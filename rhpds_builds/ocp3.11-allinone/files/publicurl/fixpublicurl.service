[Unit]
Description=Fix OpenShift publicURL
After=atomic-openshift-master-api.service

[Service]
# Note the extra-vars for oreg are required or it will fail, they are not used.
ExecStart=/usr/bin/ansible-playbook -vvi /home/cloud-user/openshift-ansible/inventory /usr/local/share/ocp-public-url-change/main.yml --extra-vars "oreg_auth_user=foo -e oreg_auth_password=foo" --extra-vars "kubevirt=false master_hostname=clientvm"
Type=simple

[Install]
WantedBy=default.target
